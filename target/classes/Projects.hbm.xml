<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.devcolibri.common.model.Projects" table="projects" schema="cdb">
        <id name="projectId" column="project_id"/>
        <property name="projectName" column="project_name"/>
        <property name="projectDescription" column="project_description"/>
    </class>
</hibernate-mapping>